<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_0.xsd"
        version="1.0">

	<message-driven name="MDBName">
		<listener-port name="mdb_listener_port_name" />
	</message-driven>
	
	<message-driven name="MDBName2">
		<listener-port name="mdb_listener_port_name_2" />
	</message-driven>
	
	<session name="SessionBean" simple-binding-name="BindingBean" />
	<session name="SessionBean2" simple-binding-name="BindingBean2" />

</ejb-jar-bnd>
